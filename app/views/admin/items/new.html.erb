<div class="container">
  <div class="row justify-content-center">
    <h3>商品新規登録</h3>
  </div>
  <div class="row justify-content-center">
    <div class="col-md-10">
      <%= form_with model: @item, url: admin_items_path, method: :post do |f| %>
        <%= render 'admin/items/item_form', f: f %>
        <div class="form-group">
          <%= f.submit :"新規登録", class: "btn btn-success" %>
        </div>
      <% end %>
    </div>
  </div>
</div>